lang ja_JP.UTF-8

set nomacatsui
set termencoding=japan

colorscheme desert
set guifont=Menlo_Regular:h13
set antialias
 
set iminsert=0 imsearch=0
 
highlight CursorIM guifg=NONE guibg=Red
highlight CursorLine ctermbg=black guibg=gray10

set transparency=10
set guioptions-=T
set lines=35
set columns=100
set cmdheight=1

setlocal omnifunc=syntaxcomplete#Complete
